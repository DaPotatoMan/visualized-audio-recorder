{
  "name": "visualized-audio-recorder",
  "version": "0.0.1",
  "author": {
    "name": "Fahadul Islam",
    "email": "dev.iamfahad@gmail.com",
    "url": "https://github.com/dapotatoman"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite --https --host --port 4000",
    "build": "tsup src/index.ts --format esm,cjs,iife --dts --minify",
    "prepare": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "dependencies": {
    "array-smooth": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.8.0",
    "tsup": "^5.11.13",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue": "^3.2.29",
    "vue-tsc": "^0.31.1"
  }
}
